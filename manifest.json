
{
    "name": " NOTE HERE",
    "version": "0.0.1",
    "manifest_version": 2,
    "offline_enabled": true,
    "background": {
     "scripts": [
        "js/background.js"
        ]
    },
    "icons": { 
        "16": "images/notepad.png" // Remove the trailing comma here
    },
    
    "browser_action": {
        "default_icon" : "images/notepad.png",
        "default_title": "NOTE HERE",
        "default_popup": "index.html"
    },
    "permissions": [
        "background", "unlimitedStorage", "tabs"
    ],
    "web_accessible_resources": [
        "index.html","js/popup.js"
    ]
}